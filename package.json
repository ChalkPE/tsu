{
  "name": "tsu",
  "version": "1.1.0",
  "description": "Watch your Twitch streamer even if you're coding",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChalkPE/tsu.git"
  },
  "keywords": [
    "twitch"
  ],
  "author": "ChalkPE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChalkPE/tsu/issues"
  },
  "homepage": "https://github.com/ChalkPE/tsu#readme",
  "configSchema": {
    "id": {
      "type": "string",
      "title": "Channel ID",
      "description": "Example: ch960116 of https://twitch.tv/ch960116",
      "default": "ch960116",
      "order": 1
    },
    "opacity": {
      "type": "number",
      "title": "Opacity",
      "description": "Opacity of Twitch player (in the background)",
      "default": 40,
      "minimum": 0,
      "maximum": 100,
      "order": 2
    },
    "foregroundOpacity": {
      "type": "number",
      "title": "Foreground opacity",
      "description": "Opacity of Twitch player (in the foreground)",
      "default": 80,
      "minimum": 0,
      "maximum": 100,
      "order": 3
    }
  }
}
